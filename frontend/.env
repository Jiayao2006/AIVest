# AIVest Frontend Environment Configuration

# API Configuration 
# When true, no API calls will be made - application will use sample data only
VITE_SAMPLE_DATA_ONLY=true

# API Base URL - where the backend API is hosted
# Used when VITE_SAMPLE_DATA_ONLY=false
VITE_API_BASE_URL=http://localhost:5000

# Environment Configuration
VITE_ENV=development

# App Configuration
VITE_APP_NAME=AIVest Banking Platform
VITE_APP_VERSION=2.0.0
